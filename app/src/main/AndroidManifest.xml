<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/buscar_icon"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@drawable/buscar_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.FuturoBuscarTelas"
        tools:targetApi="31">
        <activity
            android:name=".TelaPerfil"
            android:exported="false"
            android:label="@string/title_activity_tela_perfil"
            android:theme="@style/Theme.FuturoBuscarTelas" />
        <activity
            android:name=".TelaConsultaOS"
            android:exported="false"
            android:label="@string/title_activity_tela_consulta_os"
            android:theme="@style/Theme.FuturoBuscarTelas" />
        <activity
            android:name=".TelaPesquisarPeças"
            android:exported="false"
            android:label="@string/title_activity_tela_pesquisar_peças"
            android:theme="@style/Theme.FuturoBuscarTelas" />
        <activity
            android:name=".TelaPesquisarServicos"
            android:exported="false"
            android:label="@string/title_activity_tela_pesquisar_servicos"
            android:theme="@style/Theme.FuturoBuscarTelas" />
        <activity
            android:name=".SignUpScreen"
            android:exported="false"
            android:label="@string/title_activity_sign_up_screen"
            android:theme="@style/Theme.FuturoBuscarTelas" />
        <activity
            android:name=".TelaPesquisarOficinas"
            android:exported="false"
            android:label="@string/title_activity_tela_pesquisar_oficinas"
            android:theme="@style/Theme.FuturoBuscarTelas" />
        <activity
            android:name=".TelaInicial"
            android:exported="false"
            android:label="@string/title_activity_tela_inicial"
            android:theme="@style/Theme.FuturoBuscarTelas" />
        <activity
            android:name=".BeforeSignUpScreen"
            android:exported="false"
            android:label="@string/title_activity_before_sign_up_screen"
            android:theme="@style/Theme.FuturoBuscarTelas" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/title_activity_main"
            android:theme="@style/Theme.FuturoBuscarTelas"
            android:windowSoftInputMode="adjustResize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".onboarding.TelaPreLogin"
            android:exported="false"
            android:label="@string/title_activity_tela_pre_login"
            android:theme="@style/Theme.FuturoBuscarTelas" />
        <activity
            android:name=".LoginActivity"
            android:exported="false"
            android:theme="@style/Theme.FuturoBuscarTelas" />
    </application>

</manifest>